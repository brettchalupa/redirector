{
  "name": "@brettchalupa/redirector",
  "version": "0.1.3",
  "exports": "./mod.ts",
  "license": "MIT",
  "tasks": {
    "dev": "deno run -P --watch main.ts",
    "start": "deno run main.ts",
    "test": "deno test --allow-read --allow-write --allow-net",
    "check": "deno check **/*.ts",
    "fmt": "deno fmt",
    "lint": "deno lint",
    "check-all": "deno task check && deno task test && deno task lint && deno fmt --check"
  },
  "imports": {
    "@std/assert": "jsr:@std/assert@1",
    "@std/log": "jsr:@std/log@0.224",
    "@std/yaml": "jsr:@std/yaml@1"
  },
  "permissions": {
    "default": {
      "read": ["./config.yaml"],
      "net": true,
      "env": ["PORT", "LOG_LEVEL"]
    }
  },
  "publish": {
    "exclude": [".github/", ".claude/", "config.yaml", "CLAUDE.md"]
  }
}
